{
  "name": "electricitymaps-web",
  "version": "1.136.0",
  "description": "A real-time visualisation of the CO2 emissions of electricity consumption",
  "repository": {
    "type": "git",
    "url": "https://github.com/electricityMaps/electricitymaps-contrib.git"
  },
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "mockserver": "cd ../mockserver && pnpm run start",
    "prebuild": "pnpm run create-generated-files",
    "build": "pnpm update-version && vite build",
    "update-version": "echo '{\"version\": \"'$npm_package_version'\"}' > public/client-version.json",
    "create-generated-files": "pnpm run generate-world && pnpm run generate-zones-config",
    "generate-world": "node --import=./scripts/register-tsNodeESM.js ./geo/generateWorld.ts",
    "generate-zones-config": "node --import=./scripts/register-tsNodeESM.js ./scripts/generateZonesConfig.ts",
    "predev": "pnpm run create-generated-files",
    "dev": "vite --open",
    "cy:component": "cypress open --component -b electron",
    "cy:e2e": "cypress open --e2e -b electron",
    "cy:test-all": "cypress run && cypress run --component",
    "prepare": "cd .. && husky install web/.husky",
    "preview": "vite preview",
    "pretest": "pnpm run create-generated-files",
    "test": "vitest",
    "pretest:ci": "pnpm run create-generated-files",
    "test:ci": "vitest run",
    "format": "prettier -uw --cache --ignore-path ../.prettierignore .",
    "format:ci": "prettier --check --cache --loglevel silent --ignore-path ../.prettierignore .",
    "typecheck": "tsc",
    "lint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx .",
    "lint:ci": "eslint --cache --quiet --ignore-path .gitignore --ext .ts,.tsx . ",
    "validate-generated-files": "VERIFY_NO_UPDATES=1 pnpm run create-generated-files",
    "validate": "run-p typecheck lint:ci format:ci test:ci validate-generated-files",
    "storybook": "storybook dev -p 6006"
  },
  "lint-staged": {
    "*": "prettier -uw --cache",
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "vitest related --run --coverage=false"
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
